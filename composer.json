{
    "name": "docscentre/currency-converter",
    "description": "Docscentre coding challenge currency converter + profit tracking",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Docscentre\\CurrencyConverter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Docscentre\\CurrencyConverter\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jason Loeve",
            "email": "jasonloeve@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}








